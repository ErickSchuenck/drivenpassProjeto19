generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  credentials credentials[]
  notes notes[]
}

model credentials {
  id Int @id @default(autoincrement())
  url String 
  userName String
  password String
  title String
  owner users @relation(fields: [ownerId], references: [id])
  ownerId Int
}

model notes {
  id Int @id @default(autoincrement())
  content String
  title String
  owner users @relation(fields: [ownerId], references: [id])
  ownerId Int
}