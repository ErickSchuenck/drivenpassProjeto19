generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  credentials credentials[]
}

model credentials {
  id Int @id @default(autoincrement())
  url String 
  name String
  password String
  title String
  userId Int
  user users @relation(fields: [userId], references: [id])
}
